  "directory_structure": {
    "train": {
      "description": "Training dataset with default font",
      "raw": "data/train/raw/",
      "processed": "data/train/processed/",
      "num_samples": 10000,
      "regeneration": "python generate_dataset.py && python preprocess.py"
    },
    "font_test": {
      "description": "Test dataset using custom fonts from train_font_library/",
      "raw": "data/font_test/raw/",
      "processed": "data/font_test/processed/",
      "num_fonts": 23,
      "images_per_font": 100,
      "total_images": 2300,
      "regeneration": "python generate_font_dataset.py && python preprocess_font_dataset.py",
      "fonts_used": [
        "Barrio", "CaesarDressing", "CoralPixels", "Coustard", "GoogleSansCode",
        "Hanalei", "Inconsolata", "Jura", "LondrinaSketch", "Mansalva",
        "MooLahLah", "Plaster", "RalewayDots", "Roboto", "RubikBurned",
        "RubikGlitchPop", "RubikMicrobe", "SpecialElite", "Syncopate"
      ],
      "evaluation_results": {
        "model": "captcha_model_improved.pth",
        "overall_accuracy": "13.13%",
        "correct": 302,
        "total": 2300,
        "note": "Model trained on standard fonts shows poor performance on custom fonts"
      }
    },
    "test": {
      "description": "Reserved for test dataset",
      "raw": "data/test/raw/",
      "processed": "data/test/processed/",
      "status": "empty"
    }
  },
  "captcha_config": {
    "captcha_length": 5,
    "characters": "0-9, A-Z",
    "image_format": "PNG",
    "image_size": "60x160"
